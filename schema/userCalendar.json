{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "User Calendar",
    "type": "object",
    "properties": {
      "userEventID": {
        "type": "string"
      },
      "userID": {
        "type": "string"
      },
      "eventName": {
        "type": "string"
      },
      "eventDate": {
        "type": "string",
        "format": "date-time"
      },
      "startTime": {
        "type": "string",
        "pattern": "^([0-1]?[0-9]|2[0-3]):[0-5][0-9]$"
      },
      "endTime": {
        "type": "string",
        "pattern": "^([0-1]?[0-9]|2[0-3]):[0-5][0-9]$"
      },
      "venueID": {
        "type": "string"
      }
    },
    "required": [
      "userEventID",
      "userID",
      "eventName",
      "eventDate",
      "startTime",
      "endTime",
      "venueID"
    ]
  }
  